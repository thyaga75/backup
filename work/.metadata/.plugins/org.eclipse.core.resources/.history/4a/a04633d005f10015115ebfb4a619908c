package test;

public class Temp {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		B b= new B();
		System.out.println("B.x before main init"+ b.x);
		B bb= b;
		System.out.println("Bb.x before main init"+ bb.x);
		b.x=10;
		bb.x=20;
		System.out.println("B.x after main init"+ b.x);
		System.out.println("Bb.x after main init"+ bb.x);
		A a = new A();
		a.methodA(b);
		System.out.println("B.x after method main init"+ b.x);
		System.out.println("B.x after method main init"+ bb.x);
	}

	static class B{
		int x;
	}
	static class A{
		public void methodA(B obj){
			System.out.println("B.x before method init"+ obj.x);
			obj.x=250;
			System.out.println("B.x after method init"+ obj.x);
			obj=null;
			System.out.println("obj after method null"+ obj);
		}
	}
}
